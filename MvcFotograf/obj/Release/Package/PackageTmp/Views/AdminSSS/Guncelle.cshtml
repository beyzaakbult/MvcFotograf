@model MvcFotograf.Models.TBLSSS
@{
    ViewBag.Title = "Guncelle";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<h2>SIK SORULAN SORULARI GÜNCELLE</h2>

<script src="~/Scripts/jquery-3.4.1.min.js"></script>
<link href="~/Content/toastr.min.css" rel="stylesheet" />
<script src="~/Scripts/toastr.min.js"></script>



@using (Html.BeginForm("Guncelle", "AdminSSS", FormMethod.Post, new { enctype = "multipart/form-data" }))

{

    <div class="form-group">

        @Html.LabelFor(x => x.ID)
        @Html.TextBoxFor(x => x.ID, new { @class = "form-control" })

    </div>


    <div class="form-group">

        @Html.LabelFor(x => x.SORU)
        @Html.TextBoxFor(x => x.SORU, new { @class = "form-control" })

    </div>
    <div class="form-group">

        @Html.LabelFor(x => x.CEVAP)
        @Html.TextBoxFor(x => x.CEVAP, new { @class = "form-control" })

    </div>

    <button class="btn btn-primary">GÜNCELLE</button>
}







