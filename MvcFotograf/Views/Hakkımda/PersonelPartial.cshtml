
@using MvcFotograf.Models
@model List<TBLPERSONEL>

<section class="team-section spad">
    <div class="container">
        <div class="row">
            <div class="col-lg-6 col-md-6">
                <div class="section-title">
                    <h2>PERSONELLERİMİZ</h2>
                </div>
            </div>
            <div class="col-lg-6 col-md-6">              
            </div>
        </div>
        <div class="row">
            @foreach (var x in Model)
            {
                <div class="col-lg-3 col-md-6 col-sm-6">
                    <div class="team-item">
                        <img src="~/Resimler/@x.RESIM" alt="">
                        <div class="ti-text">
                            <h5>@x.ADSOYAD</h5>
                            <span>@x.MESLEK</span>
                            <div class="ti-social">
                                
                                <a href="@x.INSTAGRAM"><i class="fa fa-instagram"></i></a>
                            </div>
                        </div>
                    </div>
                </div>
            }
        </div>
    </div>
</section>
